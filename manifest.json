{
  "manifest_version": 2,

  "name": "metaType",
  "description": "This extension will make all your typing as cool as:\n\nM E T A\nE\nT\nA\n",
  "version": "1.0",

  "icons": {
   "128": "icon128.png",
   "16": "icon16.png",
   "48": "icon48.png"
  },

  "browser_action": {
   "default_icon": "icon16.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
